@book{swart_semicondutores_2008,
	edition = {1ª edição},
	title = {Semicondutores: Fundamentos, Técnicas e Aplicações},
	isbn = {978-85-268-0818-8},
	shorttitle = {Semicondutores},
	abstract = {Este livro traz fundamentos para entender os semicondutores e outros materiais usados em eletricidade. A resistividade é a grandeza física com maior faixa de variação na natureza, podendo ser os materiais desde supercondutores até isolantes. No meio dessa faixa encontram-se os semicondutores, que constituem materiais básicos para fabricar dispositivos e circuitos integrados. A teoria dos semicondutores constitui o foco principal deste livro, com uma discussão ampla das suas propriedades. Processos tecnológicos utilizados na obtenção de cristais de silício e na fabricação de estruturas de dispositivos e de circuitos integrados são temas de dois capítulos. O capítulo final detalha a física de junções que constituem os blocos construtivos de dispositivos semicondutores. A história, a evolução e as tendências dos dispositivos e circuitos integrados também são tratadas neste livro.},
	publisher = {Editora da Unicamp},
	author = {Swart, Jacobus W.},
	date = {2008-01-01},
}

